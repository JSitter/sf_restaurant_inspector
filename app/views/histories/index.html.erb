<p id="notice"><%= notice %></p>

<h1>Histories</h1>

<table>
  <thead>
    <tr>
      <th>Buisiness name</th>
      <th>Api</th>
      <th>Lat</th>
      <th>Long</th>
      <th>Address</th>
      <th>Zip</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @histories.each do |history| %>
      <tr>
        <td><%= history.buisiness_name %></td>
        <td><%= history.api_id %></td>
        <td><%= history.lat %></td>
        <td><%= history.long %></td>
        <td><%= history.address %></td>
        <td><%= history.zip %></td>
        <td><%= link_to 'Show', history %></td>
        <td><%= link_to 'Edit', edit_history_path(history) %></td>
        <td><%= link_to 'Destroy', history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New History', new_history_path %>
